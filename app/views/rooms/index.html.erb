<div class='container'>
  <h5> <%= current_user.email %> </h5>
  <h4> Users </h4>
  <%= turbo_stream_from 'users' %>
  <div id="users">
    <% @users.each do |user| %>
      <%= link_to user.email %>
    <% end %>
  </div>
  <h4> Rooms </h4>
    <%= turbo_stream_from "rooms" %>
  <div id="rooms">
    <%= render @rooms %>
  </div>
  <%= render 'layouts/new_room_form' %>
</div>

<div id="<%= dom_id user %>">
  <p>
    <strong>First name:</strong>
    <%= user.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= user.last_name %>
  </p>

  <p>
    <strong>Birth:</strong>
    <%= user.birth %>
  </p>

  <p>
    <strong>Sex:</strong>
    <%= user.sex %>
  </p>

  <p>
    <strong>Work:</strong>
    <%= user.work_id %>
  </p>

  <p>
    <strong>Yourself:</strong>
    <%= user.yourself %>
  </p>

  <p>
    <strong>Latitude:</strong>
    <%= user.latitude %>
  </p>

  <p>
    <strong>Longitude:</strong>
    <%= user.longitude %>
  </p>

  <p>
    <strong>Foto:</strong>  
  </p>
    <div class="foto-main-wrapper">
    <% user.foto.each do |foto| %>
       <div class="foto-preview-wrapper"><%= image_tag foto, class: "foto-preview" %></div>
    <% end %>
    </div>

</div>
